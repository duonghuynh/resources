        <criteriaItems>
            <field>AccountNumber</field>
            <operation>notEqual</operation>
            <value>1234</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingOwnerRules>
        <fullName>TM_Legal_Sharing_Rule</fullName>
        <accessLevel>Read</accessLevel>
        <accountSettings>
            <caseAccessLevel>None</caseAccessLevel>
            <contactAccessLevel>Read</contactAccessLevel>
            <opportunityAccessLevel>None</opportunityAccessLevel>
        </accountSettings>
        <description>TM Legal Sharing Rule desc ...</description>
        <label>TM Legal Sharing Rule</label>
        <sharedTo>
            <role>Legal</role>
        </sharedTo>
        <sharedFrom>
            <allInternalUsers/>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
